{
  "extends": "eslint:recommended",
  "rules": {
      "semi": ["error", "never"],
      "quotes": ["error", "double"],
      "no-console": ["error", {"allow": ["log", "warn"]}],
      "no-extra-boolean-cast": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  }
}